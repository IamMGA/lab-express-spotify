<div class="container">
  <div class="row">
    <% tracks.forEach((elem)=>{ %>
      <div class="source-element col-xs-12">
        <div class="col-xs-12 col-md-6">
          <p class="source-txt"><%= elem.name %></p>
        </div>
        <div class="col-xs-12 col-md-6 source-align">
          <audio controls>
            <source src="<%= elem.preview_url%>">
          </audio>
        </div>
      </div>
    <% }) %>
  </div>
</div>
